{
  "compilerOptions": {
    "jsx": "react",
    "module": "commonjs",
    "sourceMap": true,
    "target": "es5",
    "lib": [ "es6", "dom" ],
    "suppressImplicitAnyIndexErrors": true,
    "noImplicitReturns": false,
    "noImplicitThis": false,
    "noImplicitAny": false,
    "strictNullChecks": false,
    "experimentalDecorators": true,
    "skipLibCheck": true
  },
  "exclude": [
    "node_modules",
    "wwwroot"
  ]
}