@model AppOwnsDataShared.Models.User

<h2>
  <span><a class="btn btn-sm h2-backarrow" asp-action="Users"><i class="fa fa-backward"></i></a></span>
  <span>User Details</span>
</h2>

<form class="form-horizontal" role="form">
  <fieldset>
    <div class="form-group row">
      <label for="LoginId" class="col-sm-3 col-form-label">Login Id</label>
      <div class="col-sm-9">
        <input class="form-control " id="LoginId" name="LoginId" readonly value="@Model.LoginId">
      </div>
    </div>
    <div class="form-group row">
      <label for="UserName" class="col-sm-3 col-form-label">User Name</label>
      <div class="col-sm-9">
        <input class="form-control" id="UserName" name="UserName" readonly="readonly" value="@Model.UserName">
      </div>
    </div>
     <div class="form-group row">
      <label for="Created" class="col-sm-3 col-form-label">Created</label>
      <div class="col-sm-9">
        <input class="form-control" id="Created" name="Created" readonly="readonly" value='@Model.Created.ToString("yyyy/MM/dd hh:mm tt")'>
      </div>
    </div>
    <div class="form-group row">
      <label for="LastLogin" class="col-sm-3 col-form-label">Last Login</label>
      <div class="col-sm-9">
        <input class="form-control" id="LastLogin" name="LastLogin" readonly="readonly" value="@Model.LastLogin">
      </div>
    </div>
    <div class="form-group row">
      <label for="HomeTenant" class="col-sm-3 col-form-label">Home Tenant</label>
      <div class="col-sm-9">
        <input class="form-control" id="HomeTenant" name="HomeTenant" readonly="readonly" value="@Model.TenantName">
      </div>
    </div>
  </fieldset>
</form>